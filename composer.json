{
  "name": "navarr/verticalresponse-m2http",
  "license": "MIT",
  "description": "HttpClient and RequestProvider for navarr/verticalresponse using Magento2's HTTP Driver",
  "minimum-stability": "stable",
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "require": {
    "php": ">=5.6",
    "navarr/verticalresponse": "^2.0.2",
    "magento/framework": "^100.0",
    "guzzlehttp/psr7": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "VerticalResponse\\Client\\": "src/"
    }
  }
}
